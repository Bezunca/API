scalar DateTime
scalar ObjectID

type AuthCredentials {
  email: String!
  activated: Boolean!
}

type CEI {
  user: String!
}

type WalletsCredentials {
  cei: CEI!
}

type User {
  name: String!
  auth_credentials: AuthCredentials!
  wallets_credentials: WalletsCredentials!
}

type Query {
  user: User!
}

type Trade {
  date: DateTime!
  action: String!
  expiration: String!
  symbol: String!
  amount: Int!
  price: Int!
  full_price: Int!
  price_factor: Int!
  user_id: ObjectID!
}